{
  "expo": {
    "name": "pet-care-app",
    "slug": "pet-care-app",
    "description": "A React Native application that serves to be an all-around tool for pet owners",
    "version": "1.0.0",
    "orientation": "portrait",
    "privacy": "public",
    "icon": "./assets/PetCareLogo.png",
    "primaryColor": "#00AAFF",
    "splash": {
      "image": "./assets/PetCare.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wcandillon.fiber",
      "buildNumber": "2"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content"
    },
    "android": {
      "package": "com.wcandillon.fiber",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favi.png"
    }
  }
}
